<Window x:Name="infoDiag" x:Class="ISAO_LE_001.InfoDiag"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ISAO_LE_001"
        mc:Ignorable="d"
        Title="InfoDiag" Height="450" Width="800">
    <Grid Background="#FF3FEAC8">
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="358" Margin="10,37,0,0" VerticalAlignment="Top" Width="774">
            <TabItem Header="Red Data">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Orange Data">
                <Grid Background="#FFE5E5E5" Margin="0,-9,0.4,0"/>
            </TabItem>
            <TabItem Header="Yellow Data">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
